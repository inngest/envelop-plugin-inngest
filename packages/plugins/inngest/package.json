{
  "name": "envelop-plugin-inngest",
  "version": "0.5.4",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/inngest/envelop-plugin-inngest/blob/main/package.json",
    "directory": "packages/plugins/inngest"
  },
  "author": "A. David Thyresson <dthyresson@gmail.com>",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "require": {
        "types": "./dist/typings/index.d.cts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "default": {
        "types": "./dist/typings/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/typings/*.d.cts",
        "default": "./dist/cjs/*.js"
      },
      "import": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      },
      "default": {
        "types": "./dist/typings/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    },
    "./package.json": "./package.json"
  },
  "typings": "dist/typings/index.d.ts",
  "peerDependencies": {
    "@envelop/core": "3.0.6",
    "graphql": "14.0.0 || 15.0.0 || 16.0.0 || 16.6.0"
  },
  "dependencies": {
    "@envelop/core": "3.0.6",
    "@graphql-tools/utils": "9.2.1",
    "@whatwg-node/fetch": "0.8.8",
    "fast-json-stable-stringify": "2.1.0",
    "fast-redact": "3.2.0",
    "inngest": "1.8.4",
    "tslib": "2.5.0"
  },
  "devDependencies": {
    "@envelop/testing": "5.0.6",
    "@graphql-tools/utils": "9.2.1",
    "@types/fast-redact": "3.0.2",
    "graphql": "16.6.0",
    "typescript": "5.0.4"
  },
  "publishConfig": {
    "directory": "dist",
    "access": "public"
  },
  "sideEffects": false,
  "buildOptions": {
    "input": "./src/index.ts"
  },
  "typescript": {
    "definition": "dist/typings/index.d.ts"
  }
}
